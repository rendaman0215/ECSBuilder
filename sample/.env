# 環境設定
ENVIRONMENT=production  # 例: staging, production
AWS_REGION=ap-northeast-1  # AWSリージョン

# コンテナ設定
CONTAINER_IMAGE=nginx:latest  # 環境ごとのDockerイメージ
ENV_VAR_VALUE=example-value  # コンテナ内で使用する環境変数の値

# リソース設定
TASK_CPU=256  # タスクに割り当てるCPUリソース
TASK_MEMORY=512  # タスクに割り当てるメモリリソース

# ネットワーク設定
SUBNET_ID=subnet-0123456789abcdef0  # Fargateに割り当てるサブネットID
SECURITY_GROUP_ID=sg-0123456789abcdef0  # Fargateに割り当てるセキュリティグループID
